{
  "name": "portfolio-teg",
  "version": "0.1.0",
  "description": "Portfolio site using material-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtegtmeier/portfolio-teg.git"
  },
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/app.js",
    "clean": "rimraf build",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --colors --config webpack-production.config.js",
    "build": "npm run clean && npm run build:prod",
    "postinstall": "npm run build",
    "lint": "eslint src && echo \"eslint: no lint errors\""
  },
  "private": true,
  "author": "Jake Tegtmeier",
  "license": "MIT",
  "engines": {
    "node": "5.x"
  },
  "devDependencies": {
    "eslint": "^2.5.1",
    "eslint-plugin-react": "^4.0.0",
    "html-webpack-plugin": "^2.7.2",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "material-ui": "^0.15.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.9",
    "transfer-webpack-plugin": "^0.1.4"
  }
}
